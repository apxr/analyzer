exportPattern("^[[:alpha:]]+")
importFrom("stats", "median", "quantile", "sd")
importFrom("grid", "grid.newpage", "grid.draw")
S3method(explain, factor)
S3method(explain, character)
S3method(explain, numeric)
S3method(explain, integer, explain.numeric)
S3method(plot, analyzerPlot, plot.analyzerPlot)

